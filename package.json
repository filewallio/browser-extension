{
  "name": "filewallio",
  "version": "0.0.6",
  "description": "Filewall.io Chrome Extension",
  "author": "filewallio",
  "main": "index.js",
  "scripts": {
    "dev": "npx web-ext run --config=web-ext-config.js",
    "dev:chrome": "npx web-ext run -t chromium --config=web-ext-config.js",
    "watch:chrome": "npx webpack --env.browser=chrome --watch --progress",
    "watch:firefox": "npx webpack --env.browser=firefox --watch --progress",
    "webpack-chrome": "npx webpack --env.browser=chrome",
    "build-chrome": "npm run webpack-chrome && npx web-ext build --artifacts-dir=web-ext-artifacts/chrome",
    "postbuild-chrome": "npm run archive-source-chrome",
    "archive-source-chrome": "cross-var git archive -o web-ext-artifacts/chrome/$npm_package_name-$npm_package_version-source.zip HEAD",
    "webpack-firefox": "npx webpack --env.browser=firefox",
    "build-firefox": "npm run webpack-firefox && npx web-ext build --artifacts-dir=web-ext-artifacts/firefox",
    "postbuild-firefox": "npm run archive-source-firefox",
    "archive-source-firefox": "cross-var git archive -o web-ext-artifacts/firefox/$npm_package_name-$npm_package_version-source.zip HEAD"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filewallio/chrome-extension.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/filewallio/chrome-extension/issues"
  },
  "homepage": "https://github.com/filewallio/chrome-extension#readme",
  "engineStrict": true,
  "engines": {
    "npm": ">=5.2.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "generate-json-webpack-plugin": "^0.3.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "web-ext": "^4.1.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "common-tags": "^1.8.0",
    "cross-var": "^1.1.0",
    "rxjs": "^6.5.4",
    "web-ext": "^4.1.0",
    "webextension-polyfill": "^0.6.0"
  }
}
